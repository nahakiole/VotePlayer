{% extends "index.twig" %}

{% block content %}
    <div class="container">
        <!-- Example row of columns -->
        <div class="row">
            <div class="col-md-8 top30">

                <div class="playlist padding15">

                    {% for song in playlist %}

                    <div class="media">



                        <div class="media-body">
                            <div class="song-information">
                                {{ song.name }}
                            </div>
                        </div>
                    </div>

                    {% endfor %}
                </div>
            </div>
            <div class="col-md-4 top30">
                <div class="sidebar padding15">
                    <h2>Installation</h2>

                    <p>A small river named Duden flows by their place and
                        supplies
                        it with the necessary regelialia. It is a
                        paradisematic country, in which roasted parts of
                        sentences
                        fly into your mouth. </p>


                </div>
            </div>
        </div>

    </div>


{% endblock %}
