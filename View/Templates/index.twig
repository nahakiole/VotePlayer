<!DOCTYPE html>
<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Voteplayer - {{ title }}</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet"
          href="//netdna.bootstrapcdn.com/bootstrap/3.1.0/css/bootstrap.min.css">
    <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css"
          rel="stylesheet">
    <link rel="stylesheet" href="{{ offset }}/View/Assets/css/main.css">
    <script src="{{ offset }}/View/Assets/js/vendor/modernizr-2.6.2-respond-1.1.0.min.js"></script>
    {{ url }}
    <script type="text/javascript" src="//use.typekit.net/tef7fbw.js"></script>
    <script type="text/javascript">try {
            Typekit.load();
        } catch (e) {
        }</script>
</head>
<body>

<div class="container">

    <div class="row">
        <div class="col-md-8">
            <h1>VotePlayer</h1>
        </div>
        <div class="col-md-4 text-right user-logged-in">
            <i class="fa fa-user"></i> Robinio
        </div>
    </div>

</div>
<div class="container top15">
    <div class="navbar navbar-inverse " role="navigation">

        <div class="navbar-collapse collapse navbar-left" style="height: 1px;">
            {% import "navigation.twig" as navigationGenerator %}
            {{ navigationGenerator.menu(navigation, 'nav navbar-nav navbar-right', all) }}
        </div>
    </div>
</div>



{% block content %}{% endblock %}

<div class="clearfix"></div>



<!-- /container -->
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="../Assets/js/vendor/jquery-1.11.0.min.js"><\/script>')</script>

<script src="{{ offset }}/View/Assets/js/vendor/bootstrap.min.js"></script>

<script src="{{ offset }}/View/Assets/js/main.js"></script>
</body>
</html>
