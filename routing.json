{
    "routes": [
        {
            "action": "subdomainAction",
            "controller": "demo",
            "match": ":(?P<subdomain>[a-zA-Z0-9]+)\\.fredyframework:",
            "fullDomainMatch": true
        },
        {
            "action": "indexAction",
            "controller": "demo",
            "match": ":^/(Playlist)?$:"
        },
        {
            "action": "indexAction",
            "controller": "upload",
            "match": ":^/AddMusic:"
        },
        {
            "action": "indexAction",
            "controller": "user",
            "match": ":^/Users:"
        },
        {
            "action": "loginAction",
            "controller": "user",
            "match": ":^/Login:"
        },
        {
            "action": "indexAction",
            "controller": "settings",
            "match": ":^/Settings:"
        },
        {
            "action": "notFound",
            "controller": "error",
            "match": ":^/Error/404:"
        },
        {
            "action": "serverError",
            "controller": "error",
            "match": ":^/Error/500:"
        },
        {
            "action": "noAccess",
            "controller": "error",
            "match": ":^/Error/403:"
        }
    ]
}