{
    "routes": [
        {
            "action": "indexAction",
            "controller": "demo",
            "match": ":^/(Playlist|Home)?$:"
        },
        {
            "action": "indexAction",
            "controller": "upload",
            "match": ":^/AddMusic$:"
        },
        {
            "action": "uploadAction",
            "controller": "upload",
            "match": ":^/AddMusic/UploadHandler$:"
        },
        {
            "action": "indexAction",
            "controller": "music",
            "match": ":^/Music/JSON$:"
        },
        {
            "action": "indexAction",
            "controller": "user",
            "match": ":^/Users(/(?P<page>[0-9]+))?$:"
        },
        {
            "action": "editAction",
            "controller": "user",
            "match": ":^/Users/(Edit|Add)(/)?(?P<userid>[0-9]+)?$:"
        },
        {
            "action": "loginAction",
            "controller": "user",
            "match": ":^/Login$:"
        },
        {
            "action": "indexAction",
            "controller": "settings",
            "match": ":^/Settings$:"
        },
        {
            "action": "notFound",
            "controller": "error",
            "match": ":^/Error/404:"
        },
        {
            "action": "serverError",
            "controller": "error",
            "match": ":^/Error/500:"
        },
        {
            "action": "noAccess",
            "controller": "error",
            "match": ":^/Error/403:"
        },
        {
            "action": "registerAction",
            "controller": "user",
            "match": ":^/Register$:"
        }
    ]
}