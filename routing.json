{
    "routes": [
        {
            "action": "subdomainAction",
            "controller": "demo",
            "match": ":(?P<subdomain>[a-zA-Z0-9]+)\\.fredyframework:",
            "fullDomainMatch": true
        },
        {
            "action": "indexAction",
            "controller": "demo",
            "match": ":^/(Test/(?P<id>[0-9]+))?$:"
        },
        {
            "action": "notFound",
            "controller": "error",
            "match": ":^/Error/404:"
        },
        {
            "action": "serverError",
            "controller": "error",
            "match": ":^/Error/500:"
        },
        {
            "action": "noAccess",
            "controller": "error",
            "match": ":^/Error/403:"
        }
    ]
}